extends _layout
block var
  - metas = {}
  - metas.title = '地域詳細'
  - metas.url = 'http://XXXX.com/'
  - metas.image = 'http://XXXX.com/image.jpg'
  - metas.site_name = 'サイト名'
  - metas.description = '説明'
  include _config
  - var key = 'areaindex'
block append meta
  +meta(metas)
  include _inc_meta_facebook
  +inc_meta_facebook(metas)
block body
  include _header.pug
  div.detailKey.contentsmargin
    figure.detailKey_img
      img(src="./img/common/bnr_happymail.jpg" alt="")
