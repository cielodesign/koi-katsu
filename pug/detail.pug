      br
      | #[span.detailKey_ttl-small 出会いのプロが徹底解説!!]
  div.subrank.subcon
    div.ranknonbg
      div.contain
        article.rank_item#rank1
          div.rank_item-top
            figure
              a(href="" target="_blank")
                img(src="./img/common/icon-happymail.png" alt="")
            div.rank_item-info
              p.rank_num.rank_num01 #[span.rank_num-area.rank_num01-area 福岡]
                img(src="./img/common/icon-croun01.png" alt="")
                | 第#[span.rank_num-large 1]位
              h3.rank_subttl ハッピーメール
              p.rank_evaluation 総合評価 #[span.rank_evaluation-num 3.0] #[span.rank_evaluation-star ★★★★★]
          div.rank_item-kuchikomi
            div.rank_item-prof
              figure
                img(src="./img/common/person01.png" alt="")
              p.rank_item-name 恋活
